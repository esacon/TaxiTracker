<%- include("../templates/header", {ruta: 'Inicio'}) %>
<body>
    <main class="main">
        <div class="container-fluid text-center" style= "background-color:#FAE671;">    
            <h1><p class="text-dark">Taxi Tracker  App </p></h1>
        
            <section class="coordenadas info">                
                    <div class="row justify-content-center">
                        <div class="col" ><h5><p class="text-dark"><span id="latitud">Latitud: <%= info[0] %> ></span></p></h5></div>
                        <div class="col" ><h5><p class="text-dark"><span id="longitud">Longitud: <%= info[1] %></span></p></h5></div>
                        <div class="col" ><h5><p class="text-dark"><span id="Fecha">Fecha: <%= info[2] %></span></p></h5></div>
                        <div class="col" ><h5><p class="text-dark"><span id="Hora">Hora: <%= info[3] %></span></p></h5></div>
                    </div>            
            </section>
        </div>        
                       
        <!-- Map section -->
        <section class="map">
            <div class="container-fluid text-center" id ="mapid"></div>
        </section>

        <!-- Load mapa into de page. -->
        <script src="scripts/map.js"></script>
    </main>
<%- include("../templates/footer") %>